{% extends 'EatSleepCodeAdminBundle::admin.html.twig' %}

{% block title %}Settings | Admin{% endblock %}

{% block heading %}Settings{% endblock %}

{% block content %}
    <ul>
        {% for setting in settings %}
            <li>{{ setting.key }} - {{ setting.value }} - <a href="{{ path('admin_edit_setting', {id: setting.id}) }}">Edit</a></li>
        {% endfor %}
    </ul>
{% endblock %}
